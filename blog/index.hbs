{{!< default}}

<div class="gh-page">
    {{> "full-header"}}

    <div class="gh-postfeed">
        {{#foreach posts}}
            {{> "post-card"}}
        {{/foreach}}
    </div>

    <div class="flex flex-col gap-x-1 pt-20 pb-24 items-center">
        <img class="w-32 pb-3 rounded-full" src="{{asset "images/resting_alien.png"}}" alt="An alien working on a computer">
        <h4 class="text-body1Bold text-blackSet dark:text-blackSet-dark">
            Exciting updates in the works!
        </h4>
        <p class="text-body2 text-greyLightSet dark:text-greyLightSet-dark">
            Check back soon for the latest updates.
        </p>
    </div>
</div>
